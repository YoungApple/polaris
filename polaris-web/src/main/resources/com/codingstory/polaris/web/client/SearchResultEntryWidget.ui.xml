<ui:UiBinder
        xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g='urn:import:com.google.gwt.user.client.ui'
        xmlns:p='urn:import:com.codingstory.polaris.web.client'>
    <ui:style>
        .searchResultItem {
            margin-top: 5px;
            margin-bottom: 5px;
        }
    </ui:style>
    <g:HTMLPanel>
        <div class="{style.searchResultItem}">
            <g:SimplePanel>
                <g:Hyperlink ui:field="fileNameAnchor" />
            </g:SimplePanel>
            <p:CodeSnippetWidget ui:field="summaryCodeSnippet" />
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
