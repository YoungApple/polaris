<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <servlet>
        <servlet-name>search</servlet-name>
        <servlet-class>com.codingstory.polaris.web.server.GWTCodeSearchServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>service</servlet-name>
        <servlet-class>com.codingstory.polaris.web.server.CodeServiceServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>search</servlet-name>
        <url-pattern>/searchui/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>service</servlet-name>
        <url-pattern>/ajax/*</url-pattern>
    </servlet-mapping>
</web-app>
